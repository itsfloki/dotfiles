env:
  TERM: xterm-256color

window:
  padding:
    x: 6
    y: 6
  dynamic_padding: false
  opacity: 1.0
  title: Alacritty
  class:
    instance: Alacritty
    general: Alacritty

scrolling:
  history: 5000

font:
  normal:
    family: FiraCode Nerd Font
    style: Regular

  bold:
    family: FiraCode Nerd Font
    style: Bold

  italic:
    family: FiraCode Nerd Font
    style: Italic

  bold_italic:
    family: FiraCode Nerd Font
    style: Bold Italic

  size: 12.0

  offset:
    x: 0
    y: 1
colors:
  name: Jason Wryan
  author: ""
  primary:
    background: "#000000"
    foreground: "#ffffff"
  cursor:
    text: "#000000"
    cursor: "#ffffff"
  normal:
    black: "#222222"
    red: "#9e5641"
    green: "#6c7e55"
    yellow: "#caaf2b"
    blue: "#4c8ea1"
    magenta: "#956d9d"
    cyan: "#7c9aa6"
    white: "#909090"
  bright:
    black: "#454545"
    red: "#cc896d"
    green: "#7db37d"
    yellow: "#bfb556"
    blue: "#6bc1d0"
    magenta: "#c18fcb"
    cyan: "#8fadbf"
    white: "#ffffff"
theme: Jasonwryan
